{
    "name": "reveal/template-phpstan-compiler",
    "type": "phpstan-extension",
    "description": "Internal package for template to PHPStan-compatible compilers",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "nette/utils": "^3.2",
        "symplify/astral": ">=10.0 <10.3",
        "symplify/package-builder": "^10.2",
        "phpstan/phpstan": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^9.5",
        "symplify/easy-testing": "^10.2",
        "symplify/phpstan-extensions": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "Reveal\\TemplatePHPStanCompiler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reveal\\TemplatePHPStanCompiler\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.2-dev"
        },
        "phpstan": {
            "includes": [
                "config/services.neon"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
