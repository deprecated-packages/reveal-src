{
    "name": "reveal/reveal-neon",
    "type": "phpstan-extension",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "nette/di": "^3.0",
        "nette/utils": "^3.2",
        "nette/bootstrap": "^3.1",
        "phpstan/phpstan": "^1.4",
        "symplify/astral": "^10.0",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^9.5",
        "symplify/phpstan-extensions": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Reveal\\RevealNeon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reveal\\RevealNeon\\Tests\\": "tests"
        }
    }
}
